{
  "name": "@alisyahidin/react",
  "private": false,
  "version": "1.0.13",
  "description": "> TODO: description",
  "author": "Ali Syahidin <ali.syahidin@sg-dsa.com>",
  "license": "ISC",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "babelMacros": {
    "twin": {
      "config": "./tailwind.config.js",
      "preset": "emotion"
    }
  },
  "scripts": {
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "watch:types": "tsc --watch",
    "watch": "babel src --out-dir dist --extensions \".ts,.tsx\" --watch",
    "build:types": "tsc --build",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\"",
    "build": "yarn clean && yarn build:types && yarn build:js && node ./scripts/copy-file",
    "release": "npm run build && npm publish dist"
  },
  "dependencies": {
    "@alisyahidin/core": "^1.3.7",
    "@babel/preset-env": "^7.16.11",
    "@emotion/react": "^11.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@types/react": "^17.0.38",
    "autoprefixer": "^10.4.4",
    "postcss": "^8.4.12",
    "react": "^17.0.2",
    "tailwindcss": "^3.0.24",
    "twin.macro": "^2.8.2",
    "typescript": "^4.5.5"
  }
}
